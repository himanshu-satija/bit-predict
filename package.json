{
  "name": "bitpredict-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start:client": "cd client && yarn dev",
    "start:server": "cd server && yarn dev",
    "dev": "concurrently \"yarn start:client\" \"yarn start:server\""
  },
  "devDependencies": {
    "concurrently": "^6.2.0"
  }
}
